{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run docdb",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/docdb", // Убедитесь, что путь верный
            "cwd": "${workspaceFolder}",
            "environment": [],
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
